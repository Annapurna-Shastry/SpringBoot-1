import jenkins.model.*
jenkins = Jenkins.instance
node{
	
		stage('---CLEAN--'){
			git config --global --unset-all remote.origin.proxy
			git 'https://github.com/thogeti/SpringBoot.git'
		//	sh "mvn clean"
			
		}
		stage('---Packeage--'){			
			def mvnHome=tool name: 'Maven', type: 'maven'
			sh '${mvnHome}/bin/mvn package'
			
		}
	
	
}
